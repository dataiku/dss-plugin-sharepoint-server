{
    "meta": {
        "label": "Lists",
        "description": "Read/write lists from/to your SharePoint server",
        "icon": "icon-th-list"
    },
    "readable": true,
    "writable": false,
    "params": [
        {
            "name": "sharepoint_local",
            "label": "SharePoint preset",
            "type": "PRESET",
            "parameterSetId": "sharepoint-local-login"
        },
        {
            "name": "sharepoint_list_title",
            "label": "List title",
            "description": "(A-Z, a-z, 0-9)",
            "type": "STRING",
            "mandatory": true
        },
        {
            "name": "expand_lookup",
            "label": "Expand lookup fields",
            "description": "",
            "type": "BOOLEAN",
            "defaultValue": false,
            "mandatory": true
        },
        {
            "name": "metadata_to_retrieve",
            "label": "Metadata to retrieve",
            "type": "MULTISELECT",
            "selectChoices": [
                {
                    "value": "ID",
                    "label": "Item's ID"
                },
                {
                    "value": "Created",
                    "label": "Creation date"
                },
                {
                    "value": "Modified",
                    "label": "Modification date"
                }
            ]
        }
    ]
}